model User {
  id              String  @id @default(cuid())
  email           String  @unique
  clerkUserId     String  @unique
  stipeCustomerId String? @unique

  firsName    String?
  lastName    String?
  imageUrl    String?
  bio         String?
  website     String?
  xUrl        String?
  githubUrl   String?
  linkedInUrl String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
